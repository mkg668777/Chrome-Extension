{
  "manifest_version": 3,
  "name": "Hello World Extension",
  "version": "1.0",
  "description": "A simple Chrome extension that says hello.",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }

  "content_scripts": [
    {  
       "js": ["scripts/popup.js" "scripts/content.js"],
      "css": ["scripts/content.css"],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  
}